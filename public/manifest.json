{
  "short_name": "Visual Weather",
  "name": "Visual Weather Tab v2",
  "version": "1.0.1",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    }
  ],
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "./style/img/umbrella.png"
  },
  "permissions": [
    "http://*.google.com/",
    "https://*.google.com/",
    "geolocation",
    "http://localhost/*",
    "https://maps.googleapis.com/*"
  ],
  "manifest_version": 2
}
